{
  "env": {
    "browser": true,
    "es6": true
  },
  "extends": [
    "airbnb",
    "plugin:jsx-a11y/recommended",
    "plugin:react/recommended"
  ],
  "plugins": ["jsx-a11y", "react"],
  "rules": {
    "comma-dangle": "off",
    "import/no-extraneous-dependencies": ["error", {
      "devDependencies": true
    }],
    "no-console": "off"
  }
}